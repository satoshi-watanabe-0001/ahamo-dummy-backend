{
  "dashboard": {
    "id": null,
    "title": "Ahamo Backend - Business Metrics",
    "tags": ["ahamo", "backend", "business"],
    "timezone": "Asia/Tokyo",
    "panels": [
      {
        "id": 1,
        "title": "Total Customers",
        "type": "stat",
        "targets": [
          {
            "expr": "customers_total",
            "legendFormat": "Total Customers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Inventory Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "inventory_total_items",
            "legendFormat": "Total Items"
          },
          {
            "expr": "inventory_low_stock_items",
            "legendFormat": "Low Stock Items"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Contract Creation Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(contracts_created_total[1h])",
            "legendFormat": "Contracts/hour"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "cph"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Revenue Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "revenue_total",
            "legendFormat": "Total Revenue"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyJPY"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "5m"
  }
}
